@model IEnumerable<Student>
@{
    ViewData["Title"] = "Öğrenci Listesi";
    ViewData["Description"] = "Öğrenciler listelenir";
}

@* @{
    Html.RenderPartial("OgrenciPartial");
    
} *@
@Html.Partial("OgrenciPartial")
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Numara</th>
            <th>Sınıf</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Number</td>
                <td>@item.Class</td>
            </tr>
        }
    </tbody>
</table>

